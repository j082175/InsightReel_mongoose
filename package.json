  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "form-data": "^4.0.4",
    "googleapis": "^126.0.1",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "sharp": "^0.32.6",
    "ytdl-core": "^4.11.5"
  },
