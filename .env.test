# 테스트용 환경 변수
NODE_ENV=test
PORT=3001

# 구글 API 설정 (테스트용 - 실제 키는 넣지 마세요)
GOOGLE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"test-project"}

# Ollama 설정 (테스트 시 모킹됨)
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llava:test
USE_GEMINI=false

# 업로드 설정
MAX_FILE_SIZE=10mb
CLEANUP_DAYS=1

# 로그 설정
LOG_LEVEL=error