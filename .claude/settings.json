{
    "hooks": {
      "Stop": [
        {
          "matcher": "",
          "hooks": [
            {
              "type": "command",
              "command": "powershell.exe -c \"[System.Media.SystemSounds]::Beep.Play()\""
            }
          ]
        }
      ],
      "Notification": [
        {
          "matcher": "",
          "hooks": [
            {
              "type": "command",
              "command": "powershell.exe -c \"[System.Media.SystemSounds]::Beep.Play()\""
            }
          ]
        }
      ]
    }
  }
  